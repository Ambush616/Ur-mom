local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Niko's SS", HidePremium = false, IntroEnabled = true,IntroText = "Niko's SS", SaveConfig = true, ConfigFolder = "OrionTest"})


--Main
local MainTab = Window:MakeTab({
	Name = "Main",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})


